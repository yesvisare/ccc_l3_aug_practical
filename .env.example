# Module 9.4: Advanced Reranking Strategies
# Copy this file to .env and fill in your actual values

# Cross-encoder model configurations
# Default models used for ensemble reranking
RERANKER_MODEL_1=cross-encoder/ms-marco-MiniLM-L-6-v2
RERANKER_MODEL_2=cross-encoder/ms-marco-MiniLM-L-12-v2
RERANKER_MODEL_3=cross-encoder/ms-marco-TinyBERT-L-6

# Ensemble weights (must sum to 1.0)
ENSEMBLE_WEIGHT_1=0.4
ENSEMBLE_WEIGHT_2=0.4
ENSEMBLE_WEIGHT_3=0.2

# MMR (Maximal Marginal Relevance) parameters
MMR_LAMBDA=0.7  # Balance between relevance (higher) and diversity (lower)

# Temporal boosting parameters
RECENCY_DECAY_DAYS=30  # Half-life for exponential decay
RECENCY_BOOST_FACTOR=1.5  # Multiplicative boost for recent documents

# Performance budgets (milliseconds)
MAX_ENSEMBLE_LATENCY_MS=400
MAX_MMR_LATENCY_MS=20
MAX_TEMPORAL_LATENCY_MS=5
MAX_PERSONALIZATION_LATENCY_MS=30

# Personalization settings
MIN_USER_INTERACTIONS=100  # Minimum interactions required for personalization
PERSONALIZATION_MODEL_PATH=./models/user_preference_model.pkl

# Logging level
LOG_LEVEL=INFO
