# Module 11.3: Resource Management & Throttling
# Copy this file to .env and update with your values

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# Queue Configuration
MAX_QUEUE_SIZE_PER_TENANT=100
MAX_TOTAL_QUEUE_SIZE=5000
QUEUE_WORKERS=5
QUEUE_POLL_INTERVAL=1.0

# Quota Tier Limits (queries per hour)
FREE_TIER_HOURLY_LIMIT=100
PRO_TIER_HOURLY_LIMIT=1000
ENTERPRISE_TIER_HOURLY_LIMIT=10000

# Admin Configuration
ADMIN_API_KEY=your-secret-admin-key-here

# Throttling Settings
ENABLE_QUEUING=true
MAX_QUEUE_WAIT_SECONDS=30

# Logging
LOG_LEVEL=INFO

# Optional: Prometheus metrics
PROMETHEUS_PORT=9090
